  var employees = null;
  var titles = [];
  var names = []

  // $.getJSON( "users.json?q[site]=< % =params[:q][:site]%>", function( data ) {
  $.getJSON( "users.json", function( data ) {
    showData(data);
    // showData(data);
    employees = data;
    populate_autocomplete(data);


  });

  function populate_autocomplete(data) {
      $.each( data , function( key, val ) {
          if (titles.indexOf(data[key].title) == -1) {
            titles.push(data[key].title)
          }
          if (names.indexOf(data[key].title) == -1) {
            names.push(data[key].displayname)
          }
      });

      $( "#title" ).autocomplete({
        source: titles
      });
      $( "#name" ).autocomplete({
        source: names
      });
  }


  // $( document ).ready(function(data) {
  //     showData(employees);
  // });

  function paginate(total, per_page, current_page){

    $("#pagination").html("");



    var p = '<nav>';
        p += '<ul class="pagination">';
        p += '<li class="' + ((current_page == 1)? 'disabled' : '') + '"><a href="javascript:void(0);" onclick="show_search('+ (current_page-1) +');" aria-label="Previous"><span aria-hidden="true">&laquo;</span></a></li>';
        // p += '<li class="active"><a href="#">1 <span class="sr-only">(current)</span></a></li>';



        if (Math.ceil(total/per_page) < 5 || current_page < 5) {
           start_page = 1;
           end_page = ((Math.ceil(total/per_page) > 5) ? 5 : Math.ceil(total/per_page));

        } else {
          end_page = ((current_page == Math.ceil(total/per_page)) ? current_page : (current_page + 2 >= Math.ceil(total/per_page)) ? Math.ceil(total/per_page) : (current_page + 2));
          start_page = end_page - 4;

        }


        if (start_page > 1){
          p += '<li ><a href="javascript:void(0);" onclick="show_search('+ (start_page-1) +');">...</a></li>';
        }


        // for (i = 1; i < Math.ceil(total/per_page)+1; i++) {
        for (i = start_page; i < end_page+1; i++) {

            p += '<li class="' + ((i == current_page)? 'active' : '') + '"><a href="javascript:void(0);" onclick="show_search('+ i +');">' + i + ((i == current_page)? '<span class="sr-only">(current)</span></a></li>' : '');
        }


        if (end_page < Math.round(total/per_page)){
          p += '<li ><a href="javascript:void(0);" onclick="show_search('+ (end_page+1) +');">...</a></li>';
        }


        p += '<li class="' + ((current_page == Math.round(total/per_page))? 'disabled' : '') + '"><a href="javascript:void(0);" onclick="show_search('+ (current_page+1 > Math.round(total/per_page) ? Math.round(total/per_page) : current_page+1) +');" aria-label="Next"><span aria-hidden="true">&raquo;</span></a></li>';

        p += '</ul>';
        p += '</nav>';

    $("#pagination").append(p);

  }


  function showData(data, page = 1){

    var per_page = 25;

    $("#show").html("");

    var no_image = "data:img/png;base64,"

    var start_employee = (page - 1) * per_page;
    var end_employee = (page * per_page) ;
    var range = data.slice(start_employee,end_employee)


    $.each( range , function( key, val ) {
        // items.push( "<li id='" + key + "'>" + val + "</li>" );

        var card = '';


            card += '<div class="col-sm-6 col-md-4">';
            card += '<h4 class="card-title" style="height:2em;">' + range[key].title + '</h4>';
            card += '<h6 class="card-subtitle text-muted" style="height:1em;">' + range[key].site + '</h6>';
            card += '<div class="thumbnail">';
            card += '<img src="'+ (range[key].imagefile? range[key].imagefile : no_image)  +'" class="img-responsive img-circle"  data-holder-rendered="true" style=" display: block;">';
            card += '  <div class="caption">';
            card += '<h3>' + range[key].displayname + '</h3>';
            card += '<p><a href="#">' + range[key].mail + '</a><br/>';
            card += 'Extension: '+ range[key].ipphone + '</p>';

            <% if current_user %>
              card += '<p class="text-center"><a href="/users/'+ range[key].id +'" class="btn btn-primary" role="button">View More</a></p>';
            <% end %>

            card += '  </div>'
            card += '</div>'
            card += '</div>'



            $("#show").append(card)

    // showUser(value)
     });

     paginate(data.length,per_page, page)

  }

  function showTableData(data){

    $("#usertable > tbody").html("");

    var table = document.getElementById('usertable').getElementsByTagName("TBODY")[0];

    $.each( data, function( key, val ) {
        // items.push( "<li id='" + key + "'>" + val + "</li>" );

        var displayname = document.createElement('td');
        var title = document.createElement('td');
        var mail = document.createElement('td');
        var site = document.createElement('td');
        var extension = document.createElement('td');

        var a = document.createElement('a');
        $(a).attr('href',data[key].url).attr('id','xyz');
        a.appendChild(document.createTextNode(data[key].displayname));

        // $("#show").append("<p>" + data[key].displayname + "</p>")

        displayname.appendChild(a);
        title.appendChild(document.createTextNode(data[key].title));
        mail.appendChild(document.createTextNode(data[key].mail));
        site.appendChild(document.createTextNode(data[key].site));
        extension.appendChild(document.createTextNode(data[key].ipphone));

        var tr = document.createElement('tr');

        tr.appendChild(displayname);
        tr.appendChild(mail);
        tr.appendChild(title);

        tr.appendChild(site);
        tr.appendChild(extension);


        table.appendChild(tr);



    // showUser(value)
     });

  }

  function show_search(page = 1){

    var name = document.getElementById('name').value;
    // var mail = document.getElementById('mail').value;
    var title = document.getElementById('title').value;
    var site = document.getElementById('site').value;

    var result = [];
    // alert("sloba")
    var add;
    $.each( employees, function( key, val ) {
        if (
          ((name == '') || (name != '' && val['displayname'].toLowerCase().match(name.toLowerCase()))) &&
          ((title == '') || (title != '' && val['title'].toLowerCase().match(title.toLowerCase()))) &&
          ((site == '') || (site != '' && val['site'].toLowerCase().match(site.toLowerCase())))
      )
          result.push(val)
    });

    //
    showData(result, page);
  }

  function clearAll(){

    document.getElementById('name').value = '';
    // document.getElementById('mail').value = '';
    document.getElementById('title').value = '';
    document.getElementById('site').value = '';
    // document.getElementById('extension').value = '';
    showData(employees);

  }


</script>

