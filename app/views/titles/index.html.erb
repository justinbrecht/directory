<h1>Titles</h1>


<div class="row">
  <div class="col-xs-12 col-md-6">
    <div class="panel panel-default" >
      <div class="panel-heading">
        <h3 class="panel-title">Public</h3>
      </div>
      <div class="panel-body" id="public_titles">

        <%= render   @titles_public %>
      </div>
    </div>
  </div>
  <div class="col-xs-12 col-md-6">
    <div class="panel panel-default" >
        <div class="panel-heading">
          <h3 class="panel-title">Nonpublic</h3>
        </div>
        <div class="panel-body" id="nonpublic">
          <%= render   @titles_no_public %>
        </div>
    </div>
  </div>
</div>


<div class="text-center" style="padding-bottom: 20px;">

  <%= link_to 'New Title', new_title_path, class: "btn btn-primary" %> |

  <%= link_to 'Sync Titles', extract_titles_path, :method => :post, class: "btn btn-primary"  %>

</div>
